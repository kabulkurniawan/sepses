        owl:NamedIndividual ;
       :severityName "Error"^^xsd:string ;
	   :severityCode "3"^^xsd:int .

:warning a :severity,
        owl:NamedIndividual ;
       :severityName "Warning"^^xsd:string ;
	   :severityCode "4"^^xsd:int .

:notice a :severity,
        owl:NamedIndividual ;
       :severityName "Notice"^^xsd:string ;
	   :severityCode "5"^^xsd:int .
	   
:informational a :severity,
        owl:NamedIndividual ;
       :severityName "Informational"^^xsd:string ;
	   :severityCode "6"^^xsd:int .
	   
