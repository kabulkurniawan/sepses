@prefix : <http://purl.org/sepses/vocab/log/sysLog#>
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/sepses/vocab/log/sysLog#> a owl:Ontology .

:facilityCode a rdfs:Datatype ;
    rdfs:domain :Facility ;
    rdfs:range xsd:integer .

:facilityName a rdfs:Datatype ;
    rdfs:domain :Facility ;
    rdfs:range xsd:string .

:hasFacility a rdfs:Property ;
    rdfs:domain :Program ;
    rdfs:range :Facility .

:hasSeverity a rdfs:Property ;
    rdfs:domain :Program ;
    rdfs:range :Severity .

:processId a rdfs:Datatype ;
    rdfs:domain :Program ;
    rdfs:range xsd:int .

:programName a rdfs:Datatype ;
    rdfs:domain :Process ;
    rdfs:range xsd:string .

:severityCode a rdfs:Datatype ;
    rdfs:domain :Severity ;
    rdfs:range xsd:int .

:severityCode a rdfs:Datatype ;
    rdfs:domain :Severity ;
    rdfs:range xsd:string .

:Process a rdfs:Class .
	
:Facility a rdfs:Class .

:Severity a rdfs:Class .

:Program a rdfs:Class .



:emergency a :Severity,
       :severityCode "Emergency"^^xsd:string ;
	   :severityCode "0"^^xsd:int . 
	
:alert a :Severity,
        severityCode "Alert"^^xsd:string ;
	   :severityCode "1"^^xsd:int .
	   
:critical a :Severity,
       :severityCode "Critical"^^xsd:string ;
	   :severityCode "2"^^xsd:int .
	   
