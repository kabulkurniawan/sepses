@prefix : <http://purl.org/sepses/vocab/log/sysLog#>
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/sepses/vocab/log/sysLog#> a owl:Ontology .

:facilityCode a rdfs:Datatype ;
    rdfs:domain :facility ;
    rdfs:range xsd:integer .

:facilityName a rdfs:Datatype ;
    rdfs:domain :facility ;
    rdfs:range xsd:string .

:hasFacility a rdfs:Property ;
    rdfs:domain :program ;
    rdfs:range :facility .

:hasSeverity a rdfs:Property ;
    rdfs:domain :program ;
    rdfs:range :severity .

:programId a rdfs:Datatype ;
    rdfs:domain :program ;
    rdfs:range xsd:int .

:programName a rdfs:Datatype ;
    rdfs:domain :program ;
    rdfs:range xsd:string .

:severityCode a rdfs:Datatype ;
    rdfs:domain :severity ;
    rdfs:range xsd:int .

:severityName a rdfs:Datatype ;
    rdfs:domain :severity ;
    rdfs:range xsd:string .

:Facility a rdfs:Class .

:Severity a rdfs:Class .

:Program a rdfs:Class .



:emergency a :severity,
        owl:NamedIndividual ;
       :severityName "Emergency"^^xsd:string ;
	   :severityCode "0"^^xsd:int . 
	
:alert a :severity,
        owl:NamedIndividual ;
       :severityName "Alert"^^xsd:string ;
	   :severityCode "1"^^xsd:int .
	   
