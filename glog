@prefix glog: <http://raw.githubusercontent.com/kabulkurniawan/sepses/master/glog#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://raw.githubusercontent.com/kabulkurniawan/sepses/master/general-log> a owl:Ontology .

glog:IPAddress a owl:DatatypeProperty ;
    rdfs:domain :host;
    rdfs:range xsd:string .

glog:hostName a owl:DatatypeProperty ;
    rdfs:domain :host ;
    rdfs:range xsd:string .

glog:logFileName a owl:DatatypeProperty ;
    rdfs:domain :logSource ;
    rdfs:range xsd:string .

glog:logFilePath a owl:DatatypeProperty ;
    rdfs:domain :logSource ;
    rdfs:range xsd:string .

glog:logMessage a owl:DatatypeProperty ;
    rdfs:domain :logEntry ;
    rdfs:range xsd:string .


glog:logTimesStamp a owl:DatatypeProperty ;
    rdfs:domain :logEntry ;
    rdfs:range xsd:dateTime .


glog:originatesFrom a owl:ObjectProperty ;
    rdfs:domain :logEntry ;
    rdfs:range :logSource .

glog:producedBy a owl:ObjectProperty ;
    rdfs:domain :logSource ;
    rdfs:range :host .


glog:logSource a owl:Class .

glog:host a owl:Class .

glog:logEntry a owl:Class .
