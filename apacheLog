@prefix : <http://purl.org/sepses/vocab/log/apacheLog#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix http: <http://www.w3.org/2011/http#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/sepses/vocab/log/apacheLog#> a owl:Ontology .

:Client a rdfs:Class .

:Response a rdfs:Class .

:ResponseType a http:StatusCode .

:Request a rdfs:Class .

:Server a rdfs:Class .

:Resource a rdfs:Class .

:RequestVerb a http:Method .


:agent a rdfs:Datatype ;
    rdfs:domain :Request ;
    rdfs:range rdfs:Literal .

:belongsTo a rdfs:Property ;
    rdfs:domain :Resource ;
    rdfs:range :Server .

:giveResponse a rdfs:Property ;
    rdfs:domain :Server ;
    rdfs:range :Response .

:hasRequest a rdfs:Property ;
    rdfs:domain :Client ;
    rdfs:range :Server .

:hasResource a rdfs:Property ;
    rdfs:domain :Response ;
    rdfs:range :Resource .

:hasResponse a rdfs:Property ;
    rdfs:domain :Server ;
    rdfs:range :Client .

:hasResponseType a rdfs:Property ;
    rdfs:domain :Response ;
    rdfs:range :ResponseType .

:httpVersion a rdfs:Datatype ;
    rdfs:domain :Request ;
    rdfs:range xsd:string .

:referrer a rdfs:Datatype ;
    rdfs:domain :Request ;
    rdfs:range rdfs:Literal .

:requestTarget a rdfs:Property ;
    rdfs:domain :Request ;
    rdfs:range :Resource .

:hasRequestVerb a rdfs:Property ;
    rdfs:domain :Request ;
    rdfs:range :RequestVerb .

:resourcePath a rdfs:Datatype ;
    rdfs:domain :Resource ;
    rdfs:range rdfs:Literal .
	
:resourceSize a rdfs:Datatype ;
    rdfs:domain :Resource ;
    rdfs:range xsd:int .
	
:clientIP a rdfs:Datatype ;
    rdfs:domain :Client ;
    rdfs:range xsd:String .
