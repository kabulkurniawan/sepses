@prefix : <http://purl.org/sepses/vocab/log/baseLog#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/sepses/vocab/log/baseLog#> a owl:Ontology .


:Host a rdfs:Class .

:LogEntry a rdfs:Class .

:LogType a rdfs:Class .

:ipAddress a rdfs:Datatype ;
    rdfs:domain :Host;
    rdfs:range xsd:string .
    
:ip4Address a rdfs:Datatype ;
    rdfs:domain :Host ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf :ipAddress .
    
:ip6Address a rdfs:Datatype ;
    rdfs:domain :Host ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf :ipAddress .

:hostName a rdfs:Datatype ;
    rdfs:domain :Host ;
    rdfs:range xsd:string .

:logTypeName a rdfs:Datatype ;
    rdfs:domain :LogType ;
    rdfs:range xsd:string .

:logFilePath a rdfs:Datatype ;
    rdfs:domain :LogEntry ;
    rdfs:range xsd:string .

:logMessage a rdfs:Datatype ;
    rdfs:domain :LogEntry ;
    rdfs:range xsd:string .

:timestamp a rdfs:Datatype ;
    rdfs:domain :LogEntry ;
    rdfs:range xsd:dateTime .

:hasLogType a rdfs:Property ;
	rdfs:domain :LogEntry ;
	rdfs:range :LogType .

:originatesFrom a rdfs:Property ;
    rdfs:domain :LogEntry ;
    rdfs:range :Host .
