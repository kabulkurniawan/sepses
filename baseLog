@prefix : <http://purl.org/sepses/vocab/log/baseLog#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/sepses/vocab/log/baseLog#> a owl:Ontology .


:host a owl:Class .

:logEntry a owl:Class .

:logType a owl:Class .

:IPAddress a owl:DatatypeProperty ;
    rdfs:domain :host;
    rdfs:range xsd:string .
    
:IP4address a owl:DatatypeProperty ;
    rdfs:domain :host ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf :IPAddress .
    
:IP6address a owl:DatatypeProperty ;
    rdfs:domain :host ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf :IPAddress .

:hostName a owl:DatatypeProperty ;
    rdfs:domain :host ;
    rdfs:range xsd:string .

:logTypeName a owl:DatatypeProperty ;
    rdfs:domain :logType ;
    rdfs:range xsd:string .

:logFilePath a owl:DatatypeProperty ;
    rdfs:domain :logEntry ;
    rdfs:range xsd:string .

:logMessage a owl:DatatypeProperty ;
    rdfs:domain :logEntry ;
    rdfs:range xsd:string .

:logTimesStamp a owl:DatatypeProperty ;
    rdfs:domain :logEntry ;
    rdfs:range xsd:dateTime .

:hasLogType a owl:ObjectProperty ;
	rdfs:domain :logEntry ;
	rdfs:range :logType .

:originatesFrom a owl:ObjectProperty ;
    rdfs:domain :logEntry ;
    rdfs:range :host .
