@prefix : <http://kabulkurniawan.com/bg-knowledge/syslog-bgk#> .
@prefix sys: <http://kabulkurniawan.com/vocab/syslog-event#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:emergency a sys:severity,
        owl:NamedIndividual ;
       sys:severityName "Emergency"^^xsd:string ;
	   sys:severityCode "0"^^xsd:int . 
	
